    "# Define your processing function, processed data, and model here. \n",
    "# You may find it helpful to look through the rest of the questions first!\n",
    "copied = train.copy()\n",
    "copied['split'] = copied['email'].str.split()\n",
    "train_ham = copied[copied['spam'] == 1]\n",
    "train_ham = copied[copied['spam'] == 0]\n",
    "train_ham = train_ham.explode('split') \n",
    "train_ham = train_ham.groupby('split').size().sort_values(ascending = False)\n",
    "ham_250 = train_ham.index[:250]\n",
    "\n",
    "train_spam = copied[copied['spam']== 1]\n",
    "train_spam = train_spam.explode('split')\n",
    "train_spam = train_spam.groupby('split').size().sort_values(ascending = False)\n",
    "spam_250 = train_spam.index[:250]\n",
    "\n",
    "most_used_spam = []\n",
    "for word in spam_250:\n",
    "    if word not in ham_250:\n",
    "        most_used_spam.append(word)\n",
    "most_used_spam= most_used_spam[0:30]\n",
    "\n",
    "X_trained = words_in_texts (most_used_spam, train['email'])\n",
    "Y_trained = np.array(train['spam'])\n",
    "X_tested = words_in_texts(most_used_spam, test['email'])\n",
    "\n",
    "from sklearn.linear_model import LogisticRegression\n",
    "log_model = LogisticRegression(solver='lbfgs')\n",
    "log_model.fit(X_trained, Y_trained)\n",
    "\n",
    "log_model.score(X_trained, Y_trained)"
   ]
